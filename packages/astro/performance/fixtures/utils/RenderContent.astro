---
type Props = {
	posts: any[];
	renderer: any;
}

const { posts, renderer: ContentRenderer } = Astro.props;
---

<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width">
	<title>Md render test</title>
</head>
<body>
	{
		posts.map((entry) => {
			return (
				<div>
					<h1>{entry.data.title}</h1>
					<p>{entry.data.description}</p>
					<ContentRenderer {entry} />
				</div>
			);
		})
	}
</body>
</html>
