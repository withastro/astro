---
export async function getStaticPaths() {
	return [
		{ params: { id: 'id-1' } },
		{ params: { id: 'id-2' } }
	];
}
const { id } = Astro.params;
---
<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<title>Routing</title>
	</head>
	<body>
		<h1>[id].astro</h1>
		<p>{id}</p>
	</body>
</html>