---
export async function getStaticPaths({ routePattern }) {
    return [
        { params: { year: '2022', slug: 'post-1', routePattern } },
        { params: { year: 2022, slug: 'post-2', routePattern } },
        { params: { slug: 'post-2', year: '2022', routePattern } },
    ]
}

const { year, slug, routePattern } = Astro.params
---

<html>
  <head>
    <title>{year} | {slug}</title>
  </head>
  <body>{routePattern}</body>
</html>
