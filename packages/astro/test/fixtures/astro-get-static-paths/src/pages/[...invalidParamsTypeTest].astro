---

export function getStaticPaths ()  {
	const map = {
		// Valid types
		string: "foo",
		undefined: undefined,
		
		// Invalid types
		number: 123,
		boolean: false,
		array: [1, 2, 3],
		null: null,
		object: {a: 1},
		bigint: BigInt(123),
		function: setTimeout
	}
	const type = globalThis.getStaticPathsParamsType || "string"
	if (!map.hasOwnProperty(type)) {
		throw new Error(`Invalid type: ${type}`);
	}

	return [{
		params: {
			invalidParamsTypeTest: map[type] 
		}
	}];
};
