---
export async function getStaticPaths() {
	const pages = [
		{
			locale: 'en',
			title: 'Dynamic-only Localized Index',
		},
	];
	return pages.map(({ page, locale, title }) => {
		return {
			params: { page, locale },
			props: { title },
		};
	});
}

const { title } = Astro.props;
---

<html>
<head>
	<title>{title}</title>
</head>
<body>
<h1>{title}</h1>
</body>
</html>
