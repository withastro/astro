---
import type { GetStaticPaths } from "astro";

export const prerender = true

export const getStaticPaths = (() => {
	return [
		{ params: { title: 'foo' } },
		{ params: { title: 'bar' } },
	];
});

const { title } = Astro.params;
---


<html lang="en">
<head>
	<meta charset="utf-8"/>
	<meta name="viewport" content="width=device-width"/>
	<title>{title}</title>
</head>
<body>
<main>
	<h1>{title}</h1>
</main>
</body>
</html>
