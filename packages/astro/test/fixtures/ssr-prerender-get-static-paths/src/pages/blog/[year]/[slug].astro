---
export async function getStaticPaths() {
    return [
        { params: { year: '2022', slug: 'post-1' } },
        { params: { year: 2022, slug: 'post-2' } },
        { params: { slug: 'post-2', year: '2022' } },
    ]
}

export const prerender = true;
const { year, slug } = Astro.params
---

<html>
  <head>
    <title>{year} | {slug}</title>
  </head>
  <body></body>
</html>
