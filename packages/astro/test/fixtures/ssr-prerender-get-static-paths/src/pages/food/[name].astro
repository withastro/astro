---
export async function getStaticPaths() {
	return [
		{
			params: { name: 'tacos' },
			props: { yum: 10 },
		},
		{
			params: { name: 'potatoes' },
			props: { yum: 7 },
		},
		{
			params: { name: 'spaghetti' },
			props: { yum: 5 },
		}
	]
}

export const prerender = true;

const { yum } = Astro.props;
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<title>Food</title>
	</head>
	<body>
		<p id="url">{ Astro.url.pathname }</p>
		<p id="props">{ yum }</p>
	</body>
</html>
