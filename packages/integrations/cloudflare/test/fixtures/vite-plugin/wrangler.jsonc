{
	"compatibility_date": "2025-05-21",
  "compatibility_flags": [
  	"nodejs_compat",
    "global_fetch_strictly_public"
  ],
	"name": "astro-cloudflare-custom-entryfile",
	"main": "@astrojs/cloudflare/entrypoints/server",
	"assets": {
		"directory": "./dist",
		"binding": "ASSETS"
	},
	"images": {
		"binding": "IMAGES"
	},
	"kv_namespaces": [
    {
      "binding": "SESSION",
      "id": "SESSION"
    }
  ],
	"observability": {
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "invocation_logs": true,
      "persist": true
    }
  }
}
