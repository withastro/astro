{
  "name": "@astrojs/vercel",
  "description": "Deploy your site to Vercel",
  "version": "3.0.0-beta.1",
  "type": "module",
  "author": "withastro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/withastro/astro.git",
    "directory": "packages/integrations/vercel"
  },
  "keywords": [
    "withastro",
    "astro-adapter"
  ],
  "bugs": "https://github.com/withastro/astro/issues",
  "homepage": "https://docs.astro.build/en/guides/integrations-guide/vercel/",
  "exports": {
    "./edge": "./dist/edge/adapter.js",
    "./edge/entrypoint": "./dist/edge/entrypoint.js",
    "./serverless": "./dist/serverless/adapter.js",
    "./serverless/entrypoint": "./dist/serverless/entrypoint.js",
    "./static": "./dist/static/adapter.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "edge": [
        "dist/edge/adapter.d.ts"
      ],
      "serverless": [
        "dist/serverless/adapter.d.ts"
      ],
      "static": [
        "dist/static/adapter.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "astro-scripts build \"src/**/*.ts\" && tsc",
    "build:ci": "astro-scripts build \"src/**/*.ts\"",
    "dev": "astro-scripts dev \"src/**/*.ts\"",
    "test": "mocha --exit --timeout 20000 test/"
  },
  "dependencies": {
    "@astrojs/webapi": "^2.0.0-beta.1",
    "@vercel/nft": "^0.22.1",
    "fast-glob": "^3.2.11",
    "set-cookie-parser": "^2.5.1"
  },
  "peerDependencies": {
    "astro": "workspace:^2.0.0-beta.4"
  },
  "devDependencies": {
    "@types/set-cookie-parser": "^2.4.2",
    "astro": "workspace:*",
    "astro-scripts": "workspace:*",
    "chai": "^4.3.6",
    "mocha": "^9.2.2"
  }
}
