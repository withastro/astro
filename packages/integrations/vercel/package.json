    "dev": "astro-scripts dev \"src/**/*.ts\""
  },
  "dependencies": {
    "@astrojs/webapi": "^0.11.1",
    "@vercel/nft": "^0.18.2"
  },
  "devDependencies": {
    "astro": "workspace:*",
    "astro-scripts": "workspace:*"
  }
