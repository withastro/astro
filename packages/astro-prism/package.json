{
  "name": "@astrojs/prism",
  "version": "1.0.0",
  "description": "Supports Prism highlighting in Astro projects",
  "author": "withastro",
  "type": "module",
  "license": "MIT",
  "bugs": "https://github.com/withastro/astro/issues",
  "repository": {
