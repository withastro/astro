{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["WebWorker", "ES2021"],
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "emitDeclarationOnly": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
